{
    "name": "gurgentil/laravel-storable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Gentil",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Gurgentil\\LaravelStorable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gurgentil\\LaravelStorable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gurgentil\\LaravelStorable\\ServiceProvider"
            ],
            "aliases": {
                "DeleteCacheFile": "DeleteStorableFileFacade",
                "GenerateCacheFile": "Gurgentil\\LaravelStorable\\GenerateCacheFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
